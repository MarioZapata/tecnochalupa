<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="1" id="com.wiwili.ruleta" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Ruleta del Saber</name>
    <description>
        Ruleta del Saber
    </description>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="Orientation" value="portrait" />
    <preference name="StatusBarBackgroundColor" value="#009688" />
    <preference name="StatusBarStyle" value="blacktranslucent" />
    <preference name="ShowSplashScreen" value="false" />
</widget>
